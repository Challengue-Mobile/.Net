                    b.Navigation("Movimentacoes");
                });

            modelBuilder.Entity("API_.Net.Models.TipoUsuario", b =>
                {
                    b.Navigation("Usuarios");
                });

            modelBuilder.Entity("API_.Net.Models.Usuario", b =>
